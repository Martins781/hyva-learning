<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block template="Magebit_Example::events.phtml" />
            <block template="Magebit_Example::example.phtml" />
            <block template="Magebit_Example::privateContent.phtml" />
            <block template="Magebit_Example::cmsBlock.phtml" />
            <block class="Magento\Cms\Block\BlockByIdentifier">
                <arguments>
                    <argument name="identifier" xsi:type="string">home-page-block</argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
